%table.flat
  %thead
    %tr
      - @properties_header.each do |header|
        %th
          = header[:name]
  - @hwp_properties.each do |hwpp|
    %tr
      %td
        =hwpp.name.nil? ? "n/a" : hwpp.name
      %td
        =hwpp.unit.nil? ? "n/a" : hwpp.unit
      %td
        =hwpp.value.nil? ? "n/a" : hwpp.value
