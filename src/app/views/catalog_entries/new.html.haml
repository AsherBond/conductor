= render :partial => 'layouts/admin_nav'
= render :partial => 'section_header'

%section.admin-content-section
  %header
    %h2 Add New Catalog Entry

  .content
    = form_for @catalog_entry, :url => catalog_catalog_entries_path(@catalog), :html => {:class => 'generic'} do |f|
      = render :partial => "form", :locals => { :form => f }
