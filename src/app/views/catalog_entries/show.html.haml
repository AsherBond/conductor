= render :partial => 'layouts/admin_nav'
%header.admin-page-header
  %h1{:class => controller.controller_name}= @catalog_entry.deployable.name
  #obj_actions.button-container
    = link_to t('catalog_entries.new_catalog_entry'), new_catalog_catalog_entry_path(@catalog_entry.catalog), :class => 'button primary', :id => 'new_catalog_entry_button'
    .button-group
      = link_to t('catalog_entries.show.edit'), edit_catalog_catalog_entry_path(@catalog_entry.catalog, @catalog_entry), :class => 'button', :id => 'edit_button'
      = button_to t('catalog_entries.show.delete'), catalog_catalog_entry_path(@catalog_entry.catalog, @catalog_entry), :method => 'delete', :confirm => "#{t'catalog_entries.show.confirm_delete'}", :class => 'button danger', :id => 'delete'

%section.admin-content-section.user
  %header
    %h2= t'properties'

  .content
    %div#view
      = render :partial => 'properties'
