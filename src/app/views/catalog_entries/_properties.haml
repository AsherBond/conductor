%table
  %thead
    %tr
      %th.show
        Properties for
        = @catalog_entry.name
      %th.show
  %tbody
    %tr
      %td
        %label{:name => 'name'} Name:
      %td
        = @catalog_entry.name
    %tr
      %td
        %label{:name => 'description'} Description:
      %td
        = @catalog_entry.description
    %tr
      %td
        %label{:name => 'url'} URL:
      %td
        = link_to @catalog_entry.url, catalog_entry_path(@catalog_entry.id, :deployable_xml => true)
