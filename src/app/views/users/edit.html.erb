<div class="dcloud_form">
  <h2>Edit My Profile</h2>

  <% form_for @user, :url => account_path do |f| %>
    <%= f.error_messages %>
    <%= render :partial => "form", :object => f %>
    <%= f.submit "Update" %>
  <% end %>

  <br /><%= link_to "My Profile", account_path, :class => 'actionlink'  %>
</div>
